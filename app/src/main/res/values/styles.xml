<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MyCheckboxStyle">
        <item name="colorControlNormal">@color/your_unchecked_color</item>
        <item name="colorAccent">@color/checked_box_color</item>
    </style>

    <style name="mySwitch">
        <item name="android:colorControlActivated">@color/GREEN</item>
    </style>

    <style name="CustomChip" parent="Widget.Material3.Chip.Filter.Elevated">
        <item name="chipBackgroundColor">#126A9B</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="CustomTimePickerDialog" parent="ThemeOverlay.MaterialComponents.Dialog">
        <item name="colorPrimary">#126A9B</item> <!-- Change the color to #126A9B -->
        <item name="colorSecondary">#126A9B</item>
    </style>

    <style name="CustomAlertDialogTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <!-- Customize button colors -->
        <item name="android:background">#126A9B</item> <!-- Color of the positive button -->
        <item name="colorAccent">#126A9B</item> <!-- Color of the negative button -->
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textColorSecondary">#FFFFFF</item> <!-- Color of message text -->
        <item name="color">#FFFFFF</item>
    </style>

</resources>